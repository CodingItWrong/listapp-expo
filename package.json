{
  "name": "riverbed",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack server --config webpack.dev.js",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint src cypress/e2e",
    "lint:ci": "eslint src cypress/e2e --max-warnings=0",
    "build": "webpack --config webpack.prod.js",
    "explore:app": "source-map-explorer dist/app.*.js",
    "explore:vendors": "source-map-explorer dist/vendors.*.js",
    "deploy": "npm run build && netlify deploy --prod",
    "prepare": "husky install"
  },
  "dependencies": {
    "@codingitwrong/jsonapi-client": "^0.0.11",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.6",
    "@mui/x-date-pickers": "^6.9.0",
    "@tanstack/react-query": "^4.29.5",
    "dayjs": "^1.11.8",
    "google-maps-react": "^2.0.6",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-beforeunload": "^2.6.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.7",
    "react-native-web": "~0.18.9",
    "react-router-dom": "^6.14.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@tanstack/react-query-devtools": "^4.29.6",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "cypress": "^12.16.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.44",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.0",
    "prettier": "^2.8.8",
    "source-map-explorer": "^2.5.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
